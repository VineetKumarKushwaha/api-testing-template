{
    "name": "api-testing-template",
    "version": "1.0.0",
    "description": "Api testing template",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:ci": "FILE=Dockerfile.Prod docker-compose up --build",
        "test:dev": "docker-compose up --build"
    },
    "keywords": [
        "API",
        "Testing",
        "Javascript",
        "Node",
        "JS"
    ],
    "author": "vineetit1991@gmail.com",
    "license": "ISC",
    "devDependencies": {
        "cookie": "^0.4.1",
        "cookie-parser": "^1.4.5",
        "express": "^4.17.1",
        "jest": "^26.0.1",
        "ramda": "^0.27.0",
        "supertest": "^4.0.2",
        "yargs": "^15.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "./src/utils/setupFilesAfterEnv.js"
        ],
        "testTimeout": 30000,
        "setupFiles": [
            "./src/utils/setup.js"
        ]
    }
}